@page
@model ITVitaeSVS.UI.Web.Areas.Tags.Pages.IndexModel
@{
}

<a asp-page="CreateEdit">Create</a>

<table>
    <tr>
        <th>Name</th>
    </tr>
    @foreach (var tag in Model.Tags) {
        <tr>
            <td>@tag.Name</td>
            <td>
                <a asp-page="CreateEdit" asp-route-id="@tag.Id">Edit</a>
            </td>
        </tr>
    }
</table>