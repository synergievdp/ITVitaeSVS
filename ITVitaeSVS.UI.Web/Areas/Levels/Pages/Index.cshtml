@page
@model ITVitaeSVS.UI.Web.Areas.Levels.Pages.IndexModel
@{
}

<a asp-page="CreateEdit">Create</a>

<table>
    <tr>
        <th>Name</th>
    </tr>
    @foreach (var level in Model.Levels) {
        <tr>
            <td>@level.Name</td>
            <td>
                <a asp-page="CreateEdit" asp-route-id="@level.Id">Edit</a>
            </td>
        </tr>
    }
</table>